{
    "version": 2,
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/static/index.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/app.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ],
